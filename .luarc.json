{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT",
    "pathStrict": true
  },
  "workspace": {
    "library": [
      "${3rd}/luv/library"
    ],
    "checkThirdParty": "Disable"
  },
  "diagnostics": {
    "globals": ["vim"],
    "disable": ["undefined-field", "need-check-nil"]
  }
}
